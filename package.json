{
    "name": "nexus",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@hookform/resolvers": "^3.6.0",
        "@reduxjs/toolkit": "^2.5.0",
        "@tanstack/react-query": "^5.62.8",
        "@types/jest": "^27.5.2",
        "@types/node": "^16.18.101",
        "@types/react": "^18.3.3",
        "@types/react-dom": "^18.3.0",
        "antd": "^4.24.16",
        "antd-mask-input": "^2.0.7",
        "axios": "^1.7.2",
        "jwt-decode": "^4.0.0",
        "lodash-es": "^4.17.21",
        "moment": "^2.30.1",
        "postcss-cli": "^9.1.0",
        "react": "^18.3.1",
        "react-datepicker": "^4.8.0",
        "react-dnd": "^16.0.1",
        "react-dnd-html5-backend": "^14.1.0",
        "react-dom": "^18.3.1",
        "react-error-boundary": "^3.1.4",
        "react-hook-form": "^7.52.0",
        "react-i18next": "^11.15.1",
        "react-redux": "^9.1.2",
        "react-router-dom": "^6.24.0",
        "react-scripts": "5.0.1",
        "recharts": "^2.12.7",
        "recoil": "^0.7.7",
        "redux": "^5.0.1",
        "redux-persist": "^6.0.0",
        "redux-thunk": "^3.1.0",
        "sass": "^1.77.6",
        "typescript": "^4.9.5",
        "utf8": "^3.0.0",
        "web-vitals": "^2.1.4",
        "xlsx": "^0.18.5",
        "yup": "^1.4.0"
    },
    "scripts": {
        "dev": "npm-run-all -p watch:* start",
        "start": "react-scripts start",
        "build": "npm-run-all -p build:* && react-scripts build",
        "build-dev": "env-cmd -f .env.dev react-scripts build",
        "build-test": "env-cmd -f .env.test react-scripts build",
        "build-stg": "env-cmd -f .env.staging react-scripts build",
        "build-prod": "env-cmd -f .env.production react-scripts build",
        "test": "react-scripts test",
        "build:scss": "sass --no-source-map --style=compressed src/styles/app.scss:src/styles/app.css",
        "watch:scss": "sass --style=compressed --watch src/styles/app.scss:src/styles/app.css",
        "build:css": "postcss src/styles/index.scss -o src/styles/tailwind.css",
        "watch:css": "postcss src/styles/index.scss -o src/styles/tailwind.css --watch",
        "eject": "react-scripts eject"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@types/lodash-es": "^4.17.12",
        "@types/utf8": "^3.0.3",
        "autoprefixer": "^10.4.19",
        "env-cmd": "^10.1.0",
        "npm-run-all": "^4.1.5",
        "postcss": "^8.4.39",
        "prettier": "3.3.2",
        "tailwindcss": "^3.4.4"
    }
}
